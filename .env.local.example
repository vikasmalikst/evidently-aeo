# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Onboarding Flow (Brand/Competitors) - Post-Login
# Skip onboarding after login (skip brand/competitors, go directly to setup)
VITE_SKIP_ONBOARDING_AFTER_LOGIN=false

# Force onboarding after login (always show brand/competitors flow)
VITE_FORCE_ONBOARDING_AFTER_LOGIN=false

# Setup Flow (Welcome/Models/Topics/Prompts)
# Force setup to show (regardless of localStorage state)
VITE_FORCE_SETUP=false

# Skip setup checks (for testing dashboard without setup)
VITE_SKIP_SETUP_CHECK=false

# Jump directly to a specific setup step
# Options: welcome, models, topics, prompts
VITE_SETUP_STEP=

# Skip setup after login (redirects directly to dashboard after login)
VITE_SKIP_SETUP_AFTER_LOGIN=false

# Force setup after login (always redirects to setup after login)
VITE_FORCE_SETUP_AFTER_LOGIN=false

# Testing & Development
# Enable testing mode in Dashboard (shows topic modal for testing)
VITE_ENABLE_TESTING_MODE=false

# Legacy flags (for backward compatibility - can be removed)
VITE_FORCE_ONBOARDING=false
VITE_SKIP_ONBOARDING_CHECK=false
VITE_ONBOARDING_STEP=
