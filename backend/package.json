{
  "name": "answerintel-backend",
  "version": "1.0.0",
  "description": "AnswerIntel Backend Services - TypeScript/Node.js/Express.js",
  "main": "dist/app.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/app.js",
    "dev": "ts-node-dev --respawn --transpile-only src/app.ts",
    "dev:safe": "./start-dev.sh",
    "cleanup": "./cleanup-ports.sh",
    "scores:compute": "ts-node-dev --transpile-only src/scripts/computeVisibilityScores.ts",
    "positions:extract": "ts-node --transpile-only src/scripts/extract-positions.ts",
    "keywords:generate": "ts-node --transpile-only src/scripts/generate-keywords-for-existing.ts",
    "citations:extract": "ts-node --transpile-only src/scripts/extractCitations.ts",
    "citations:extract:simple": "ts-node --transpile-only src/scripts/extractCitationsSimple.ts",
    "citations:categorize": "ts-node --transpile-only src/scripts/categorize-citations-for-existing.ts",
    "sentiments:score": "ts-node --transpile-only src/scripts/score-sentiment.ts",
    "cron:scoring:scheduler": "ts-node --transpile-only src/cron/scoringScheduler.ts",
    "cron:scoring:worker": "ts-node --transpile-only src/cron/scoringWorker.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:e2e": "jest --config jest.e2e.config.js",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix"
  },
  "keywords": [
    "answerintel",
    "aeo",
    "query-generation",
    "ai-collectors",
    "typescript",
    "express",
    "authentication"
  ],
  "author": "AnswerIntel Team",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0",
    "axios": "^1.12.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "openai": "^4.104.0",
    "uuid": "^9.0.1",
    "cron-parser": "^4.9.0",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
